{
  "name": "AgentOrange",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Make Internet Great Again",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["http://*/*", "http://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png"
  },
  "permissions": [ "tabs", "http://*/*", "http://*/*"]
}
